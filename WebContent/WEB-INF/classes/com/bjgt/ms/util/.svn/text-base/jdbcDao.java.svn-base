	public static String buteToStr(byte[] b)
			throws UnsupportedEncodingException {
		return new String(b, "utf-8");
	}

	public static int byteToInt2(byte[] b) {

		int mask = 0xff;
		int temp = 0;
		int n = 0;
		for (int i = 0; i < 14; i++) {
			n <<= 8;
			temp = b[i] & mask;
			n |= temp;
		}
		return n;
	}


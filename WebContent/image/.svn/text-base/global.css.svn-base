.mainnav li a:hover {
	background: #E8E8E8;
	text-decoration: none;
	width: 101%;
	background: -moz-linear-gradient(center top, rgba(255, 255, 255, 1) 1%,
		rgba(243, 243, 243, 1) 100%) repeat scroll 0 0 rgba(0, 0, 0, 0);
	border-radius: 2px;
	border-right: none;
	color: #717171;
	display: inline-block;
}

.mainnav li a:focus {
	outline: none;
}

.mainnav li a {
	border-bottom: 1px solid #c4c4c4;
	border-top: 1px solid white;
	box-shadow: 0 1px 0 rgba(255, 255, 255, 1);
	box-sizing: border-box;
	color: #3f3f3f;
	float: left;
	font-size: 14px;
	height: 38px;
	line-height: 38px;
	position: relative;
	width: 100%;
}

.mainnav ul li .sub li a {
	border-bottom: 1px solid #c4c4c4;
	border-top: 1px solid white;
	box-shadow: 0 1px 0 rgba(255, 255, 255, 1);
	box-sizing: border-box;
	color: #3f3f3f;
	float: left;
	font-size: 14px;
	height: 38px;
	line-height: 38px;
	position: relative;
	width: 100%;
}

.mainnav .hasDrop {
	padding-right: 0 !important;
	position: absolute;
	right: 10px;
	top: 5px;
	height: 10px;
	width: 30px;
	color: #000;
}

.mainnav ul li .sub li:last-child {
	border-bottom: 1px solid #c4c4c4;
	box-shadow: 0 1px 0 rgba(255, 255, 255, 1);
}

.mainnav ul li .sub li {
	width: 100%;
}

.mainnav ul li .sub li a:hover {
	width: 101%;
}

.mainnav ul li .sub li:first-child a.current {
	border-top: medium none;
}

.mainnav ul li .sub li a.current {
	border-bottom: 1px solid #c4c4c4;
	box-shadow: 0 1px 0 rgba(255, 255, 255, 1);
}

.mainnav ul li .sub li a {
	border-bottom: 1px solid transparent;
	border-top: 1px solid transparent;
	box-shadow: none;
	font-size: 13px;
	padding-left: 30px;
}

.mainnav ul li .sub li a.current:visited {
	border-bottom: 1px solid #c4c4c4;
	box-shadow: 0 1px 0 rgba(255, 255, 255, 1);
	width: 101%;
	border-right: none;
}

.mainnav ul li .sub li a.current:focus {
	outline: none;
}

.mainnav ul li .sub.expand {
	display: block;
}

.mainnav ul li .sub {
	display: none;
	float: left;
	margin-top: 0;
	width: 100%;
}

.mainnav ul {
	list-style: none outside none;
	margin: 0;
}


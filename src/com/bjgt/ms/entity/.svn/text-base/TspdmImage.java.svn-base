 */

public class TspdmImage extends BaseObject {

	private static final long serialVersionUID = 2214874545218811L;
	private String pageno;// 页码
	private byte[] spimage;// 商品页码图片

	public TspdmImage() {
		super();
	}

	public TspdmImage(String pageno, byte[] spimage) {
		super();
		this.pageno = pageno;
		this.spimage = spimage;
	}

	public String getPageno() {
		return pageno;
	}

	public void setPageno(String pageno) {
		this.pageno = pageno;
	}

	public byte[] getSpimage() {
		return spimage;
	}

	public void setSpimage(byte[] spimage) {
		this.spimage = spimage;
	}

	@Override
	public String toString() {
		return "TspdmImage [pageno=" + pageno + ", spimage="
				+ Arrays.toString(spimage) + "]";
	}

}